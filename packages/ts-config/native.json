{
  "extends": "./base.json",
  "compilerOptions": {
    "jsx": "react-native",
    "jsxImportSource": "react",
    "lib": [
      "dom",
      "esnext"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}